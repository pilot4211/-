using System;

internal class Program
{
    static void Main(string[] args)
    {
        string name = "Лапшин";
        string surname = "Дмитрий";
        string temporary;

        Console.WriteLine($"Имя - {name} / Фамилия - {surname}");
        temporary = name;
        name = surname;
        surname = temporary;
        Console.WriteLine($"Имя - {name} / Фамилия - {surname}");
    }
}
