using System;

internal class Program
{
    static void Main(string[] args)
    {
       string name = " Лапшин ";
       string surname = " Дмитрий ";

        Console.WriteLine(name + surname);

        if (name == " Дмитрий ")
        {
            
        }

        if (name == " Лапшин ")
        {
            name = " Дмитрий ";
        }

        if (surname == " Лапшин ")
        {

        }

        if (surname == " Дмитрий ")
        {
            surname = " Лапшин ";
        }

         Console.WriteLine(name + surname);
    }
}
