using System;
internal class Program
{
    static void Main(string[] args)
    {
        int pictures = 52;
        int pageСapacity = 3;

        int completedpages = pictures / pageСapacity;
        int remainder = pictures % pageСapacity;

        Console.WriteLine($"Заполненых страниц - {completedpages} \nостаток - {remainder}");
    }
}
