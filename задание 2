using System;

internal class Program
{
    static void Main(string[] args)
    {
        string name = "Лапшин";
        string surname = "Дмитрий";
        string speed;

        Console.WriteLine($"Имя - {name} / Фамилия - {surname}");
        speed = name;
        name = surname;
        surname = speed;
        Console.WriteLine($"Имя - {name} / Фамилия - {surname}");
    }
}
